{
    "name": "lukaszaleckas/laravel-fallback-cache",
    "autoload": {
        "psr-4": {
            "LaravelFallbackCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFallbackCache\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "authors": [
        {
            "name": "lukaszaleckas",
            "email": "luk.zaleckas@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.11",
        "phpstan/phpstan-mockery": "^1.1.2",
        "phpunit/phpunit": "^9.6.19",
        "slevomat/coding-standard": "^8.15.0",
        "squizlabs/php_codesniffer": "^3.9.1",
        "larastan/larastan": "^1.0.3",
        "orchestra/testbench": "^6.43.2"
    }
}
